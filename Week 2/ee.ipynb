   "source": [
    "#導入庫\n",
    "import sympy as sym\n",
    "from sympy import symbols, cos, exp, latex\n",
    "from IPython.display import display, Math\n",
    "\n",
    "#建立x未知數物件\n",
    "x = symbols('x')\n",
    "\n",
    "#建立函式物件\n",
    "exp_q2 = sym.sqrt(1+x**2)\n",
    "\n",
    "#求導\n",
    "result_q2 = sym.diff(exp_q2)\n",
    "\n",
    "#輸出Latex很漂亮\n",
    "display(Math(r\"f'(x) = \" + latex(result_q2)))"
   ]
  }
 ],
